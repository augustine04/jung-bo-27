#include <stdio.h>
#include <queue>
#define max 1000

int n, k, G[max+1][max+1];
int v[max+1];
std::queue<int> Q;
void BFS(int t){
    Q.push(t);
    while(!Q.empty()){
        t = Q.front();
        Q.pop();
        if(v[t] == 0){
            printf("%d ", t);
            v[t] = 1;
            for(int i = 1; i<=n; i++){
                if(G[t][i] == 1 && v[i] == 0){
                    Q.push(i);
                }
            }
        }
    }
}

int main(){
    int s, e;
    scanf("%d %d", &n, &k);
    for(int i = 1; i <= k; i++){
        scanf("%d %d", &s, &e);
        G[s][e] = G[e][s] = 1;
    }
    BFS(1);
    printf("\n");
}
